{
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true,
    ".vite": true,
    "node_modules": true,
    "dist": true
  },
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "cSpell.enableFiletypes": [
    "!javascript",
    "!javascriptreact",
    "!json",
    "!scss",
    "!typescript"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true
  }
}
