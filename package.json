{
  "name": "@macrostrat/web",
  "private": true,
  "version": "5.1.1",
  "description": "Macrostrat map interface",
  "type": "module",
  "scripts": {
    "bundle": "vite build",
    "clean": "rm -rf dist/*",
    "dev": "yarn run server:dev",
    "build": "vite build",
    "server": "node ./server/index.js",
    "server:dev": "node --env-file=.env ./server/index.js",
    "format": "prettier --write src packages",
    "server:prod": "NODE_ENV=production npm run server",
    "dev:storybook": "yarn workspace @macrostrat/storybook run dev",
    "start": "yarn run build && yarn run server"
  },
  "workspaces": [
    "deps/web-components/packages/*",
    "deps/web-components/storybook",
    "deps/cesium-viewer",
    "deps/cesium-martini",
    "packages/*"
  ],
  "authors": [
    "John Czaplewski <jczaplewski@wisc.edu>",
