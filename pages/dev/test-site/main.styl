.nav
  position: fixed
  width: 100%
  z-index: 10000
  top: 0px
  left: 0px
  
  ul 
    list-style-type: none;
    margin: 0;
    padding: 0;
    padding-left: 20%
    overflow: hidden;
    background-color: #015eab;

  li 
    float: left;
    font-size: 15px

  li a 
    display: block;
    color: white;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none


.footer 
  display grid
  grid-template-columns 50% 50%
  background-color white
  text-align center
  color #363434

  ul 
    list-style-type: none

  a 
    color black
    font-weight bold
  a:hover
    color black

.footer-text
  color #363434
  font-size 24px
  font-weight 500

.footer-links
  display grid
  margin auto

.col-sm-4
  color: white
  width: 33.333%
  color: #E0E1E6;
  font-weight: 200;
  text-align: center;
  min-height: 1px;
  padding-left: 15px;
  padding-right: 15px;
  font-weight: bold
  padding-bottom: 18px

